public class ExRuntimeException extends RuntimeException {

	/**
	 * @param string
	 */
	public ExRuntimeException(String string) {
		super(string);
	}

	/**
	 * @param e
	 */
	public ExRuntimeException(Exception e) {
		super(e);
	}

	/**
	 * 
	 */
	private static final long serialVersionUID = 4506123808402853131L;

}
